{
  "name": "@asgard-js/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "serve:react-demo": "nx run react-demo:serve",
    "lint:core": "nx run @asgard-js/core:lint",
    "test:core": "nx run @asgard-js/core:test",
    "test:core:watch": "cd packages/core && npm run test:watch",
    "test:core:ui": "cd packages/core && npm run test:ui",
    "test:core:coverage": "cd packages/core && npm run test:coverage",
    "build:core": "nx run @asgard-js/core:build --skip-nx-cache --verbose",
    "watch:core": "nx build @asgard-js/core --watch",
    "lint:react": "nx run @asgard-js/react:lint",
    "test:react": "nx run @asgard-js/react:test",
    "test:react:watch": "cd packages/react && npm run test:watch",
    "test:react:ui": "cd packages/react && npm run test:ui",
    "test:react:coverage": "cd packages/react && npm run test:coverage",
    "build:react": "nx run @asgard-js/react:build --skip-nx-cache --verbose",
    "watch:react": "nx build @asgard-js/react --watch",
    "test": "nx run-many --target=test --projects=@asgard-js/core,@asgard-js/react",
    "test:watch": "nx run-many --target=test --projects=@asgard-js/core,@asgard-js/react --watch",
    "test:coverage": "nx run-many --target=test --projects=@asgard-js/core,@asgard-js/react --coverage",
    "lint:packages": "nx run-many --target=lint --projects=@asgard-js/core,@asgard-js/react",
    "release:core": "nx run @asgard-js/core:nx-release-publish",
    "release:react": "nx run @asgard-js/react:nx-release-publish"
  },
  "private": true,
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "6.11.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@eslint/js": "^9.8.0",
    "@nx/devkit": "20.4",
    "@nx/eslint": "20.4",
    "@nx/eslint-plugin": "20.4",
    "@nx/js": "20.4",
    "@nx/playwright": "20.4",
    "@nx/react": "20.4",
    "@nx/vite": "20.4",
    "@nx/web": "^20.8.2",
    "@playwright/test": "^1.36.0",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.3.12",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@testing-library/react": "15.0.6",
    "@types/node": "18.16.9",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.3.1",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "jsdom": "~22.1.0",
    "nx": "20.4",
    "prettier": "^2.6.2",
    "sass": "^1.55.0",
    "semver": "^7.7.2",
    "tslib": "^2.3.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.13.0",
    "verdaccio": "^5.0.4",
    "vite": "^5.0.0",
    "vite-plugin-dts": "~3.8.1",
    "vitest": "^1.3.1"
  },
  "workspaces": [
    "libs/**",
    "packages/**",
    "apps/**"
  ],
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "nx": {
    "includedScripts": [],
    "targets": {
      "local-registry": {
        "executor": "@nx/js:verdaccio",
        "options": {
          "port": 4873,
          "config": ".verdaccio/config.yml",
          "storage": "tmp/local-registry/storage"
        }
      }
    }
  }
}
